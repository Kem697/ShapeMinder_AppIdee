<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="searchview_Card" parent="Widget.Material3.SearchBar">
        <item name="cardCornerRadius">50dp</item>
        <item name="strokeColor">#000000</item>
        <item name="strokeWidth">2dp</item>
        <item name="android:hint">Suche</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="color">@color/white</item>
        <item name="navigationIconTint">@color/black</item>
        <item name="android:backgroundTint">#FDECCE</item>
    </style>



    <style name="content_Card" parent="CardView">
        <item name="cardCornerRadius">5dp</item>
        <item name="strokeColor">#CECECE</item>
        <item name="strokeWidth">2dp</item>
        <item name="navigationIconTint">@color/white</item>
        <item name="android:backgroundTint">@color/black</item>
    </style>


    <style name="settings_options_card" parent="CardView">
        <item name="cardCornerRadius">10dp</item>
        <item name="strokeColor">#CECECE</item>
        <item name="strokeWidth">1dp</item>
        <item name="navigationIconTint">@color/white</item>
        <item name="android:backgroundTint">@color/black</item>
    </style>


    <style name="small_content_Card" parent="CardView">
        <item name="cardCornerRadius">10dp</item>
        <item name="strokeColor">#CECECE</item>
        <item name="strokeWidth">2dp</item>
        <item name="navigationIconTint">@color/white</item>
        <item name="android:backgroundTint">@color/white</item>
    </style>



    <style name="tab_navigation" parent="Widget.AppCompat.ActionBar.TabBar">
        <item name="strokeColor">#CECECE</item>
        <item name="strokeWidth">2dp</item>
        <item name="navigationIconTint">@color/white</item>
        <item name="tabIndicatorColor">@color/tertiary</item>
        <item name="android:backgroundTint">@color/black</item>
    </style>


    <style name="tab_items_in_navigation" parent="Widget.AppCompat.ActionBar.TabBar">
        <item name="color">@color/white</item>
    </style>


    <style name="content_CardDecription" parent="CardView">
        <item name="cardCornerRadius">10dp</item>
        <item name="strokeColor">#CECECE</item>
        <item name="strokeWidth">2dp</item>
        <item name="navigationIconTint">@color/white</item>
        <item name="android:backgroundTint">#FEF5E7</item>
    </style>



    <style name="transparent" parent="CardView">
        <item name="android:backgroundTint">#00FEF5E7</item>
    </style>




</resources>