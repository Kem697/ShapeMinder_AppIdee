<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="customUserInput" parent="Widget.Material3.CardView.Outlined">
        <item name="android:layout_width">260dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="cardBackgroundColor">#FDECCE</item>
        <item name="strokeColor">#D1CDCD</item>
        <item name="android:fontFamily">"@font/roboto"</item>
        <item name="strokeWidth">4dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:textColorHint">#434141</item>
    </style>


    <style name="customUserInputMaterial" parent="Widget.Material3.CardView.Outlined">
        <item name="android:background">#FDECCE</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">#000000</item>
        <item name="fontFamily">"@font/roboto"</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:textColorHint">#434141</item>
    </style>


    <style name="customButton" parent="android:Widget.TextView">
        <item name="cornerRadius">10dp</item>
        <item name="android:backgroundTint">#000000</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="fontFamily">"@font/roboto"</item>
        <item name="android:textAlignment">center</item>
    </style>




</resources>